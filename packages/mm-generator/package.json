{
  "name": "mm-generator",
  "version": "0.0.1",
  "description": "CLI to generate objective-cpp boilerplate for React Native native modules.",
  "keywords": [
    "react-native",
    "library",
    "objective-c",
    "swift"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/callstack/react-native-builder-bob.git",
    "directory": "packages/mm-generator"
  },
  "bugs": {
    "url": "https://github.com/callstack/react-native-builder-bob/issues"
  },
  "homepage": "https://callstack.github.io/react-native-builder-bob/build",
  "bin": "bin/mm-generator",
  "files": [
    "bin",
    "lib"
  ],
  "engines": {
    "node": ">= 18.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "prepare": "babel --extensions .ts,.tsx src --out-dir lib --source-maps --delete-dir-on-start"
  },
  "dependencies": {
    "@react-native/codegen": "0.74.0",
    "chalk": "^4.0.0",
    "fs-extra": "^10.1.0",
    "yargs": "^17.5.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.18.5",
    "@babel/plugin-proposal-class-properties": "^7.17.12",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-typescript": "^7.17.12",
    "@types/babel__core": "^7.1.19",
    "@types/fs-extra": "^9.0.13",
    "@types/yargs": "^17.0.10"
  }
}
